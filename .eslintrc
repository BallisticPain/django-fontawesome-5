{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": "error",
    "max-classes-per-file": 0,
    "prefer-destructuring": 0,
    "no-underscore-dangle": 0,
    "no-return-assign": 0,
    "class-methods-use-this": 0
  }
}